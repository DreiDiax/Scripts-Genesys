select
	to_char(participantstart,'DD/MM/YYYY') as fecha,
	conversationid,
	participantname,
	participantindex,
	transferfromqueue,
	transfertoqueue
from
	gns_purecloud.transfer_agent
where
	transferfromqueue in ('Conmutador Institucional', 'Conmutador Central Citas - Ext. 7988')
	and participantindex >= 1
